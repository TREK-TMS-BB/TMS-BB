<?xml version="1.0"?>
<!-- terminals and variables are not disjoint -->
<CFG>
    <variables>
		<var>A</var>
		<var>B</var>
        <var>X</var>
        <var>Q</var>
        <var>R</var>
        <var>S</var>
        <var>T</var>
    </variables>
    <terminals>
        <t>a</t>
        <t>b</t>
    </terminals>
    <rules>
        <r var="A" body="a"/>
        <r var="B" body="b"/>
        <r var="X" body="AA"/>
        <r var="X" body="BB"/>
        <r var="X" body="SA"/>
        <r var="X" body="TB"/>
        <r var="Q" body="AA"/>
        <r var="Q" body="SA"/>
        <r var="R" body="BB"/>
        <r var="R" body="TB"/>
        <r var="S" body="AQ"/>
        <r var="T" body="BR"/>
    </rules>
    <start>X</start>
</CFG>
